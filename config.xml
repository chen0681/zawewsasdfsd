<?xml version='1.0' encoding='utf-8'?>
<widget id="com.fs.sourceview" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Source View</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <plugin name="cordova-plugin-device" version="1.0.1" />
    <plugin name="cordova-plugin-file" version="2.1.0" />
    <plugin name="cordova-plugin-file-transfer" version="1.2.1" />
    <plugin name="cordova-plugin-network-information" version="1.0.1" />
    <plugin name="cordova-plugin-splashscreen" version="2.1.0" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
</widget>
